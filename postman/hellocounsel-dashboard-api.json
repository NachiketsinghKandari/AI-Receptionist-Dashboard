{
	"info": {
		"_postman_id": "hellocounsel-dashboard-api",
		"name": "HelloCounsel Dashboard API",
		"schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
	},
	"auth": {
		"type": "bearer",
		"bearer": [
			{
				"key": "token",
				"value": "{{access_token}}",
				"type": "string"
			}
		]
	},
	"variable": [
		{
			"key": "base_url",
			"value": "http://localhost:3000"
		},
		{
			"key": "email",
			"value": ""
		},
		{
			"key": "password",
			"value": ""
		},
		{
			"key": "access_token",
			"value": ""
		}
	],
	"item": [
		{
			"name": "Auth",
			"item": [
				{
					"name": "Login",
					"event": [
						{
							"listen": "test",
							"script": {
								"type": "text/javascript",
								"exec": [
									"const response = pm.response.json();",
									"if (response.access_token) {",
									"    pm.collectionVariables.set(\"access_token\", response.access_token);",
									"}"
								]
							}
						}
					],
					"request": {
						"auth": {
							"type": "noauth"
						},
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"email\": \"{{email}}\",\n  \"password\": \"{{password}}\"\n}"
						},
						"url": {
							"raw": "{{base_url}}/api/auth/login",
							"host": ["{{base_url}}"],
							"path": ["api", "auth", "login"]
						}
					}
				}
			]
		},
		{
			"name": "Calls",
			"item": [
				{
					"name": "List Calls",
					"request": {
						"method": "GET",
						"url": {
							"raw": "{{base_url}}/api/calls?env=staging&limit=20&offset=0&firmId=1&callType=inbound&startDate=2025-01-01&endDate=2025-01-31&search=example&sortBy=created_at&sortOrder=desc",
							"host": ["{{base_url}}"],
							"path": ["api", "calls"],
							"query": [
								{ "key": "env", "value": "staging", "disabled": true },
								{ "key": "limit", "value": "20", "disabled": true },
								{ "key": "offset", "value": "0", "disabled": true },
								{ "key": "firmId", "value": "1", "disabled": true },
								{ "key": "callType", "value": "inbound", "disabled": true },
								{ "key": "startDate", "value": "2025-01-01", "disabled": true },
								{ "key": "endDate", "value": "2025-01-31", "disabled": true },
								{ "key": "search", "value": "example", "disabled": true },
								{ "key": "sortBy", "value": "created_at", "disabled": true },
								{ "key": "sortOrder", "value": "desc", "disabled": true }
							]
						}
					}
				},
				{
					"name": "Get Call by ID",
					"request": {
						"method": "GET",
						"url": {
							"raw": "{{base_url}}/api/calls/:id?env=staging",
							"host": ["{{base_url}}"],
							"path": ["api", "calls", ":id"],
							"query": [
								{ "key": "env", "value": "staging", "disabled": true }
							],
							"variable": [
								{ "key": "id", "value": "1" }
							]
						}
					}
				},
				{
					"name": "List Flagged Calls",
					"request": {
						"method": "GET",
						"url": {
							"raw": "{{base_url}}/api/calls/flagged?env=staging&limit=20&offset=0&firmId=1&flagType=silence",
							"host": ["{{base_url}}"],
							"path": ["api", "calls", "flagged"],
							"query": [
								{ "key": "env", "value": "staging", "disabled": true },
								{ "key": "limit", "value": "20", "disabled": true },
								{ "key": "offset", "value": "0", "disabled": true },
								{ "key": "firmId", "value": "1", "disabled": true },
								{ "key": "flagType", "value": "silence", "disabled": true }
