					"request": {
						"method": "GET",
						"url": {
							"raw": "{{base_url}}/api/cekura/call-mapping?startDate=2025-01-01T00:00:00Z&endDate=2025-01-02T00:00:00Z&environment=staging&page=1&pageSize=25&fetchAll=false",
							"host": ["{{base_url}}"],
							"path": ["api", "cekura", "call-mapping"],
							"query": [
								{ "key": "startDate", "value": "2025-01-01T00:00:00Z" },
								{ "key": "endDate", "value": "2025-01-02T00:00:00Z" },
								{ "key": "environment", "value": "staging", "disabled": true },
								{ "key": "page", "value": "1", "disabled": true },
								{ "key": "pageSize", "value": "25", "disabled": true },
								{ "key": "fetchAll", "value": "false", "disabled": true }
							]
						}
					}
				},
